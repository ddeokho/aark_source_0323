            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:scale_height="80"
            app:scale_left="50"
            app:scale_textsize="40"
            app:scale_top="380"
            app:scale_width="300" />

        <Button
            android:id="@+id/select_ksae_baja_btn"
