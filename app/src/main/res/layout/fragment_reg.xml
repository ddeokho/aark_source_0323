            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/khnphdbold"
            android:text="*공지 예정"
            android:textAlignment="textStart"
            android:textStyle="bold"
            app:scale_height="80"
            app:scale_left="80"
            app:scale_textsize="30"
            app:scale_top="1140"
