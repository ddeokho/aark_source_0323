        android:textAlignment="textStart"
        android:textStyle="bold"
        app:scale_height="80"
        app:scale_left="40"
        app:scale_textsize="35"
        app:scale_top="670"
        app:scale_width="1000" />


        <Button
            android:id="@+id/select_sae_baja_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/round_button"
            android:text="Baja 규정"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:scale_textsize="40"
            app:scale_top="750"
            app:scale_left="50"
            app:scale_width="300"
            app:scale_height="80"/>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/khnphdbold"
            android:text="그린카 대회 : 9/6(금)~7(토)"
            android:textAlignment="viewStart"
            android:textStyle="bold"
            app:scale_height="80"
            app:scale_left="40"
            app:scale_textsize="45"
            app:scale_top="890"
            app:scale_width="1000" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/khnphdbold"
            android:text="1. 참가팀 접수 마감(자율주행부분) : 4/12"
            android:textAlignment="textStart"
            android:textStyle="bold"
            app:scale_height="80"
