            app:scale_textsize="45"
            app:scale_top="40"
            app:scale_width="1000" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/khnphdbold"
            android:text="1. 참가팀 접수 마감 : 4/1~4/30"
            android:textAlignment="textStart"
            android:textStyle="bold"
            app:scale_height="80"
            app:scale_left="35"
            app:scale_textsize="40"
            app:scale_top="120"
            app:scale_width="1000" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
