            android:textStyle="bold"
            app:scale_height="80"
            app:scale_left="35"
            app:scale_textsize="40"
            app:scale_top="300"
            app:scale_width="1000" />


        <Button
            android:id="@+id/select_ksae_formula_btn"
            android:layout_width="wrap_content"
